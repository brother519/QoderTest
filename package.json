{
  "name": "file-upload-service",
  "version": "1.0.0",
  "description": "File upload and storage service with S3 and CloudFront",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": ["file-upload", "s3", "cloudfront", "fastify"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fastify": "^4.25.2",
    "@fastify/multipart": "^8.1.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^7.2.4",
    "@fastify/rate-limit": "^9.1.0",
    "@aws-sdk/client-s3": "^3.478.0",
    "@aws-sdk/lib-storage": "^3.478.0",
    "@aws-sdk/s3-request-presigner": "^3.478.0",
    "@aws-sdk/cloudfront-signer": "^3.478.0",
    "mongoose": "^8.0.3",
    "sharp": "^0.33.1",
    "file-type": "^18.7.0",
    "dotenv": "^16.3.1",
    "envalid": "^8.0.0",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^20.10.6",
    "@types/node-cron": "^3.0.11",
    "tsx": "^4.7.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "prettier": "^3.1.1"
  }
